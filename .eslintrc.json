{
  "extends": ["react-app"],
  "rules": {
    "indent": ["error", 4],
    "comma-dangle": "off",
    "one-var": "off",
    "one-var-declaration-per-line": "off",
    "eol-last": "off",
    "no-param-reassign": "off",
    "class-methods-use-this": "off",
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
  }
}